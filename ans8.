#include <iostream>
using namespace std;

int main() {
    int n, m;
    cin >> n >> m;  
    int arr[n][m];
    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            cin >> arr[i][j];

    int primarySum = 0;
    int secondarySum = 0;

    for (int i = 0;i < n; i++) {
        primarySum += arr[i][i];
        secondarySum += arr[i][n - i - 1]; 
    }
    cout << "Primary diagonal sum = " << primarySum << endl;
    cout << "Secondary diagonal sum = " << secondarySum << endl;
    return 0;
}
