#include <iostream>
using namespace std;

int staircase(int n) {
    if (n <= 1)
        return 1;  
    int prev1 = 1; 
    int prev2 = 1; 
    int curr = 0;
    for (int i = 2; i <= n; i++) {
        curr = prev1 + prev2; 
        prev2 = prev1;
        prev1 = curr;
    }
    return curr;
}
int main() {
    int n;
    cin >> n;
    cout << staircase(n);
    return 0;
}


