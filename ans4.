#include<iostream>
using namespace std;
int main(){
    int n;
    cin >> n;
    int nums[n];
    for(int i=0 ; i< n ; i++){
        cin >> nums[i];
    }
    int start = 0 , end = n-1;
    while( start < end){
        swap( nums[start], nums[end]);
        start++;
        end --;
    }
    for(int i=0 ; i< n ; i++){
        cout<< nums[i]<<" ";
    }
}
