#include <iostream>
using namespace std;

int findSum(int arr[], int n) {
    if (n == 0)
        return 0;
    return arr[n - 1] + findSum(arr, n - 1);
}
int main() {
    int n;
    cin >> n;
    int arr[n];
    for (int i = 0; i < n; i++)
        cin >> arr[i];
    cout << findSum(arr, n);
    return 0;
}
