#include <iostream>
#include <vector>
using namespace std;

int main() {
    int m, n;
    cin >> m >> n;
    int arr[m][n];
    for (int i = 0; i < m; i++)
        for (int j = 0; j < n; j++)
            cin >> arr[i][j];

    vector<int> spiral;
    int top = 0, bottom = m - 1;
    int left = 0, right = n - 1;

    while (top <= bottom && left <= right) {
        for (int j = left; j <= right; j++)
            spiral.push_back(arr[top][j]);
        top++;
        for (int i = top;i <= bottom; i++)
            spiral.push_back(arr[i][right]);
        right--;
        if (top <= bottom) {
            for (int j = right;j >= left; j--)
                spiral.push_back(arr[bottom][j]);
            bottom--;
        }
        if (left <= right) {
            for (int i = bottom; i >= top; i--)
                spiral.push_back(arr[i][left]);
            left++;
        }
    }
    for (int i = 0; i < spiral.size(); i++) {
        cout << spiral[i];
        if (i != spiral.size() - 1) cout << " ";
    }
    return 0;
}
