#include<iostream>
using namespace std;
int main(){
    int n;
    cin >> n;
    int nums[n];
    for(int i=0 ; i< n ; i++){
        cin >> nums[i];
    }
    long long expectedSum = (long long) (n *(n+1)) /2;
    long long actualSum = 0;
    for (int i = 0 ; i< n ; i++){
        actualSum += nums[i];
    }
    cout << (expectedSum - actualSum)<<endl;
    return 0 ;
}
